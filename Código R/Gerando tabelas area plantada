    h2017 <- h2017[,c(7,1,2,3,4,5,6)]

#Juntando todas as tabelas:

    area_plantada <- rbind(h2003,h2004,h2005,h2006,h2007,h2008,h2009,
        h2010,h2011,h2012,h2013,h2014,h2015,h2016,h2017)

#Exportando

    write.table(area_plantada,file='area_plantada.csv',sep=';',dec=".",na="0",quote=TRUE, row.names=FALSE)