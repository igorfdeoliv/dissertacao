#Juntando todas as tabelas

    quantidade_produzida_t <- rbind(q2003,q2004,q2005,q2006,q2007,q2008,q2009,
        q2010,q2011,q2012,q2013,q2014,q2015,q2016,q2017)

#Exportando

     write.table(quantidade_produzida_t,file='quantidade_produzida.csv',sep=';',dec=".",na="0",quote=TRUE, row.names=FALSE)